{
  "name": "Sandy Desert",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#666666"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Purple Ink",
      "scope": [
        "string",
        "constant.language.boolean.false",
        "constant.language.boolean.true",
        "constant.numeric",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#988FD6"
      }
    },
    {
      "name": "#ff0000",
      "scope": [ "keyword.other.debugger", "support.function.console.js", "support.class.console.js" ],
      "settings": {
        "foreground": "#fe2c2d"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Variable",
      "scope": [ "variable.language", "variable.other" ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#999999"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#999999"
      }
    },
    {
      "name": "Normal Text",
      "scope": [ "entity.name.class", "entity.name.type", "entity.name.function", "punctuation.definition.variable" ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Embedded code markers",
      "scope": [ "punctuation.section.embedded.begin", "punctuation.section.embedded.end" ],
      "settings": {
        "foreground": "#fe2c2d"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [ "constant.language", "meta.preprocessor" ],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Support.construct",
      "scope": [ "support.function.construct", "keyword.other.new" ],
      "settings": {
        "foreground": "#fe2c2d"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [ "constant.character", "constant.other" ],
      "settings": {
        "foreground": "#e96934"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {}
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {}
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [ "punctuation.definition.tag.begin", "punctuation.definition.tag.end" ],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Continuation",
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#fe2c2d"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {}
    },
    {
      "name": "Library class/type",
      "scope": [ "support.type", "support.class" ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Library Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#e96934"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {}
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {}
    },
    {
      "name": "diff: header",
      "scope": [ "meta.diff", "meta.diff.header" ],
      "settings": {
        "background": "#ffd34a",
        "fontStyle": "italic",
        "foreground": "#152212"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "background": "#2a2411",
        "fontStyle": "",
        "foreground": "#d02623"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "background": "#2a2411",
        "fontStyle": "",
        "foreground": "#e96934"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "background": "#2a2411",
        "foreground": "#6eded3"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [ "markup.bold" ],
      "settings": {
        "foreground": "#999999",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [ "markup.italic" ],
      "settings": {
        "foreground": "#999999",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#5ed5cc"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#cccccc"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "fontStyle": "",
        "foreground": "#cccccc"
      }
    }
  ],
